[Unit]
Description=MJPEG Server
[Service]
User=hpclab
# The configuration file application.properties should be here:
#change this to your workspace
WorkingDirectory=/home/hpclab/optimized-mjpeg-server/src
#path to executable. 
#executable is a bash script which calls jar file
ExecStart=/home/hpclab/optimized-mjpeg-server/src/main.py
SuccessExitStatus=0
TimeoutStopSec=10
Restart=always
RestartSec=5
[Install]
WantedBy=multi-user.target
